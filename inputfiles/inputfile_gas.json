{
    "io_settings": {
        "csv_output_file": "./output/out.csv",
        "auxiliary_info": true,
        "auxiliary_info_file": "./output/auxiliary_info.md",
        "sankey_plot": "default",
        "csv_time_unit": "date",
        "csv_output_keys": "nothing",
        "return_output_keys": "all_incl_flows",
        "output_plot": "all_incl_flows"
    },
    "simulation_parameters": {
        "start": "01.01.2024 00:00",
        "end": "07.01.2024 23:45",
        "start_end_unit": "dd.mm.yyyy HH:MM",
        "time_step": 900,
        "time_step_unit": "seconds",
        "epsilon": 1e-7,
        "weather_file_path": "M:/SIZ 2020/S20203 Hafner_KliEn/AB 4 Energiekonzept/AP 4.2 Auslegung & Bewertung Energieerzeugungsanlagen/GenSim/Wetterdatensatz mit Verschiebung/TRY_476921091528/TRY2015_Konstanz_Jahr.dat"

    },
    "components": {
        "Grid_IN": {
            "type": "FlexibleSupply",
            "output_refs": [
                "BUS_Power"
            ],
            "medium": "m_power",
            "is_source": true,
            "constant_power": 5.0e6
        },
        "natural_gas": {
            "type": "FixedSupply",
            "output_refs": [
                "BUS_gas"
            ],
            "medium": "m_gas",
            "constant_supply": 9999999,
            "scale": 1
        },
        "FuelBoiler_Gas": {
            "type": "FuelBoiler",
            "output_refs": [
                "BUS_Heat"
            ],
            "m_fuel_in": "m_gas",
            "m_heat_out": "m_heat",
            "power_th": 6.5e6,
            "efficiency_fuel_in": "const:1.1",
            "efficiency_heat_out": "const:1.0",
            "linear_interface": "heat_out",
            "min_power_fraction": 0.1,
            "output_temperature": 90
        },
        "BHKW_Gas": {
            "type": "CHPP",
            "output_refs": [
                "BUS_Power",
                "BUS_Heat"
            ],
            "power_el": 2.0e6,
            "__OPTIONAL_MEDIA__": "",
            "m_fuel_in": "m_c_g_natgas",
            "m_el_out": "m_power",
            "m_heat_out": "m_heat",
            "__OPTIONAL_EFFICIENCIES__": "",
            "linear_interface": "fuel_in",
            "efficiency_fuel_in": "const:1.0",
            "efficiency_el_out": "pwlin:0.01,0.17,0.25,0.31,0.35,0.37,0.38,0.38,0.38",
            "efficiency_heat_out": "pwlin:0.8,0.69,0.63,0.58,0.55,0.52,0.5,0.49,0.49",
            "nr_discretization_steps": 8,
            "min_power_fraction": 0.1,
            "output_temperature": 90
        },
        "BUS_gas": {
            "type": "Bus",
            "medium": "m_gas",
            "connections": {
                "input_order": [
                    "natural_gas"
                ],
                "output_order": [
                    "BHKW_Gas",
                    "FuelBoiler_Gas"
                ],
                "energy_flow": [
                    [1, 1]
                ]
            }
        },
        "BUS_Power": {
            "type": "Bus",
            "medium": "m_power",
            "connections": {
                "input_order": [
                    "BHKW_Gas",
                    "Grid_IN"
                ],
                "output_order": [
                    "Demand_Power"
                ],
                "energy_flow": [
                    [1],
                    [1]
                ]
            }
        },
        "BUS_Heat": {
            "type": "Bus",
            "medium": "m_heat",
            "connections": {
                "input_order": [
                    "BHKW_Gas",
                    "FuelBoiler_Gas"
                ],
                "output_order": [
                    "Demand_Heat"
                ],
                "energy_flow": [
                    [1],
                    [1]
                ]
            }
        },
        "Demand_Heat": {
            "type": "Demand",
            "output_refs": [],
            "medium": "m_heat",
            "energy_profile_file_path": "./profiles/MA/germersheim_heat.prf",
            "constant_temperature": 90,
            "scale": 1
        },
        "Demand_Power": {
            "type": "Demand",
            "output_refs": [],
            "medium": "m_power",
            "energy_profile_file_path": "./profiles/MA/germersheim_power.prf",
            "scale": 1
        } 
    }
