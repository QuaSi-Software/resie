            "power_el": 5000,
            "linear_interface": "fuel_in",
            "efficiency_fuel_in": "const:1.0",
            "efficiency_el_out": "const:0.4",
            "efficiency_heat_out": "const:0.6"
        },
        "TST_01_HZG_01_HTP": {
            "type": "HeatPump",
            "output_refs": [
                "TST_01_HZG_01_BUS"
            ],
            "control_modules": [
                {
                    "name": "storage_driven",
                    "high_threshold": 0.5,
                    "low_threshold": 0.1,
                    "storage_uac": "TST_01_HZG_01_BFT"
                }
            ],
            "power_th": 20000,
            "constant_cop": 3.0
        },
        "TST_01_HZG_01_BFT": {
            "type": "BufferTank",
            "output_refs": [
                "TST_01_HZG_01_BUS"
            ],
            "model_type": "ideally_stratified",
            "capacity": 40000,
            "initial_load": 0.5
