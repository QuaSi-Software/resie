                    "name": "economical_discharge",
                    "pv_threshold": 750.0,
                    "min_charge": 0.2,
                    "discharge_limit": 0.05,
                    "pv_plant_uac": "TST_01_ELT_01_PVP",
                    "battery_uac": "TST_01_ELT_01_BAT"
                }
            ],
            "capacity": 10000,
            "load": 5000
