{
    "io_settings": {
        "csv_output_file": "./output/out.csv",
        "auxiliary_info": true,
        "auxiliary_info_file": "./output/auxiliary_info.md",
        "sankey_plot": "default",
        "csv_output_keys": {
			"TST_HP_01": [
                "m_h_w_lt1 IN",
                "m_h_w_ht1 OUT",
                "m_e_ac_230v IN",
                "COP"
            ],
            "TST_DEM_01": ["Temperature"],
            "TST_SRC_01": ["Temperature", "Max_Energy"]
        },
        "output_plot": {
            "1": {
                "key": {"TST_HP_01": ["m_h_w_lt1 IN"]},
                "axis": "left",
                "unit": "kWh",
                "scale_factor": 0.001
            },
            "2": {
                "key": {"TST_HP_01": ["m_h_w_ht1 OUT"]},
                "axis": "left",
                "unit": "kWh",
                "scale_factor": 0.001
            },
            "3": {
                "key": {"TST_HP_01": ["m_e_ac_230v IN"]},
                "axis": "left",
                "unit": "kWh",
                "scale_factor": 0.001
            },
            "4": {
                "key": {"TST_HP_01": ["COP"]},
                "axis": "left",
                "unit": "-",
                "scale_factor": 1
            },
            "5": {
                "key": {"TST_DEM_01": ["Temperature"]},
                "axis": "right",
                "unit": "°C",
                "scale_factor": 1
            },
            "6": {
                "key": {"TST_SRC_01": ["Temperature"]},
                "axis": "right",
                "unit": "°C",
                "scale_factor": 1
            },
            "7": {
                "key": {"TST_SRC_01": ["Max_Energy"]},
                "axis": "left",
                "unit": "kWh",
                "scale_factor": 0.001
            }
        }    
    },
    "simulation_parameters": {
        "start": 0,
        "end": 86400,
        "time_step_seconds": 900
    },
    "components": {
        "TST_DEM_01": {
            "type": "Demand",
            "medium": "m_h_w_ht1",
            "control_refs": [],
            "output_refs": [],
            "energy_profile_file_path": "./profiles/tests/demand_heating_energy.prf",
            "temperature_profile_file_path": "./profiles/tests/demand_heating_temperature.prf",
            "scale": 1500
        },
        "TST_SRC_01": {
            "type": "BoundedSupply",
            "medium": "m_h_w_lt1",
            "control_refs": [],
            "output_refs": [
                "TST_HP_01"
            ],
            "max_power_profile_file_path": "./profiles/tests/source_heat_max_power.prf",
            "temperature_profile_file_path": "./profiles/tests/source_heat_temperature.prf",
            "scale": 6000
        },
        "TST_GRI_01": {
            "type": "GridConnection",
            "medium": "m_e_ac_230v",
            "control_refs": [],
            "output_refs": [
                "TST_HP_01"
            ],
            "is_source": true
        },
        "TST_HP_01": {
            "type": "HeatPump",
            "control_refs": [
                "TST_DEM_01"
            ],
            "output_refs": [
                "TST_DEM_01"
            ],
            "strategy": {
                "name": "demand_driven"
            },
            "power_th": 9000,
            "constant_cop": 3.0,
            "min_power_fraction": 0.0
        }
    }
}